<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.mapper.BankTypePromptMapper">
	<resultMap type="BankTypePrompt" id="BankTypePromptResultMap">
			<id column="ID"  property="typeId" />
			<result column="TYPE_PROMPT"  property="typePrompt" />
	</resultMap>
	<select id="listPageTypePrompt" resultMap="BankTypePromptResultMap" parameterType="BankTypePrompt">
	   SELECT e.* FROM  bank_type_prompt e where  ID  is not null ORDER BY ID DESC
	</select>

	<select id="getTypePromptByid" parameterType="String" resultMap="BankTypePromptResultMap">
			select * from bank_type_prompt where ID=#{typeId,jdbcType=VARCHAR}
	</select>

 	 <update id="updateTypePrompt" parameterType="BankTypePrompt">
			update bank_type_prompt set  TYPE_PROMPT=#{typePrompt} where ID=#{typeId,jdbcType=VARCHAR}
	</update> 
	
</mapper>