<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.mapper.ImportHtmlMapper">
 
	<select id="selectImportListPage" resultType="com.app.entity.ImportHtml" parameterType="com.app.entity.ImportHtml" >
        select
	      b.html_id as htmlId,
	      b.html_url as htmlUrl,
	      b.html_date as htmlDate,
	      b.html_user as htmlUser,
	      b.html_name as htmlName
	     from bank_import_html b
	     ORDER BY html_id desc
	</select> 
	 
	<insert id="saveImportHtml" parameterType="com.app.entity.ImportHtml">
	   insert into bank_import_html (html_url,html_date,html_user,html_name) 
	    values(#{htmlUrl,jdbcType=VARCHAR},#{htmlDate,jdbcType=VARCHAR},
	    #{htmlUser,jdbcType=VARCHAR},#{htmlName,jdbcType=VARCHAR})
	</insert>
	
	 
	
</mapper>