<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.mapper.ShopPraiseMapper">
		 <resultMap type="ShopPraise" id="shoppraiseResultMap">
			<id column="praise_id" property="praiseid" />
			<result column="praise_count" property="praisecount" />
			<result column="click_count" property="clickcount" />
			<result column="praise_radix_count" property="praiseradixcount" />
			<result column="click_radix_count" property="clickradixcount" />
			<result column="odds_shop_id" property="oddsshopid" />
	</resultMap> 


	 <select id="findAllShopPraise" resultType="ShopPraise" resultMap="shoppraiseResultMap">
  		select u.praise_id,u.praise_count,u.click_count,u.praise_radix_count,u.click_radix_count,u.odds_shop_id from bank_odds_shop_praise u
	</select>
	
	<insert id="insertShopPraise" parameterType="ShopPraise"
		keyProperty="oddsshopid">
	insert into bank_odds_shop_praise(praise_count,click_count,praise_radix_count,click_radix_count,odds_shop_id)values
	(#{praisecount},#{clickcount},#{praiseradixcount},#{clickradixcount},#{oddsshopid,jdbcType=VARCHAR})
	</insert>
	 
	<delete id="deleteShopPraise" parameterType="String">
		delete from bank_odds_shop_praise where odds_shop_id=#{oddsshopid,jdbcType=VARCHAR}
	</delete>
</mapper>
