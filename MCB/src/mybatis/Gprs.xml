<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.mapper.GprsMapper">
		 <resultMap type="Gprs" id="gprsResultMap">
			<id column="city_id" property="cityid" />
			<result column="city_no" property="cityno" />
			<result column="city_name" property="cityname" />
	</resultMap> 


    <select id="getGprsById" parameterType="String" resultMap="gprsResultMap">
		select * from bank_city_gps t where t.city_no=#{cityno,jdbcType=VARCHAR} 
	</select>


	 <select id="findAllGprs"  resultMap="gprsResultMap">
  		select s.city_no,s.city_name from bank_city_gps s order by convert(city_name using gbk) asc 
	</select>
	<select id="getGprs" resultMap="gprsResultMap">
  			select * from bank_city_gps order by city_one asc
	</select>
</mapper>
